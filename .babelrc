{
  "presets": [
    "es2015",
    "react",
    "flow",
    [
      "env",
      {
        "targets": {
          "node": true
        }
      }
    ]
  ],
  "ignore": [
    "MockData"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ],
  "retainLines": true
}